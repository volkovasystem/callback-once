{
  "name": "@volkovasystem/callback-once",
  "version": "0.0.1",
  "description": "Single execution callback wrapper.",
  "main": "callback-once.js",
  "scripts": {
    "test": "node ./callback-once.test.js",
    "clean": "rm -Rfv ./node_modules && rm ./callback-once.js",
    "compress": "npx terser ./callback-once.module.js --compress --comments all --output ./callback-once.js",
    "modularize": "npx babel --plugins @babel/plugin-transform-modules-umd ./callback-once.js --out-file ./callback-once.js",
    "minify": "npx terser ./callback-once.js --compress --comments all --output ./callback-once.js",
    "build": "npm run compress && npm run modularize && npm run minify",
    "deploy": "npm run clean && npm install && npm run build && npm test",
    "prepublishOnly": "npm run clean && npm install && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/volkovasystem/callback-once.git"
  },
  "keywords": [
    "callback",
    "once",
    "callback-once"
  ],
  "author": "Richeve S. Bebedor <richeve.bebedor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/volkovasystem/callback-once/issues"
  },
  "homepage": "https://github.com/volkovasystem/callback-once#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-modules-umd": "^7.9.0"
  }
}
